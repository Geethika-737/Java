import java.io.*;
public class Main{
    public static void main(String args[]){
        String str="Geethika";
        System.out.printf("\nThe string value is: %s\n",str);
        int x=512;
        System.out.printf("\nThe integer value is: %d\n",x);
        double f=5.246718;
        System.out.printf("Thedouble value is:%f\n",f);
        System.out.printf("After formatting to specific width:the value is:%.4f\n",f);
        System.out.printf("After formatting to right margin the value is:%10.2f\n",f);
    }
}
output
The string value is: Geethika

The integer value is: 512
Thedouble value is:5.246718
After formatting to specific width:the value is:5.2467
After formatting to right margin the value is:      5.25
